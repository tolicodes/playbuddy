  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build:version:sync --platform all && eas build --non-interactive --platform all",
    "build:simulator:android": "eas build --platform android --profile development-simulator",
    "build:simulator:ios": "eas build --platform ios --profile development-simulator",
    "submit": "eas submit --platform all",
    "prebuild": "npx expo prebuild --clean",
    "run:simulator": "eas build:run -p ios --latest",
    "update": "eas update --branch production --message ",
    "postinstall": "patch-package",
    "launch-deeplink": "sh -c 'npx uri-scheme open --ios \"playbuddy://https://l.playbuddy.me/$1\"' --"
  },
