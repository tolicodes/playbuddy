{
  "name": "playbuddy-scraper",
  "private": true,
  "description": "Node.js app with TypeScript for Cloud Run",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/app.js",
  "bin": {
    "cmd": "./bin/cmd"
  },
  "files": [
    "dist/**.*"
  ],
  "scripts": {
    "clean": "rimraf dist types",
    "dev": "NODE_ENV=development source .env && tsx watch src/app.ts",
    "start": "NODE_ENV=production node ./dist/app.js",
    "script:qr": "tsx src/scripts/qrcodes/index.ts",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "fill-missing-data": "tsx src/scripts/fillMissingData.ts",
    "deploy": "flyctl secrets import < .env.production && fly deploy --ha=false",
    "docker": "docker build -t playbuddy-scraper . && npm run docker:run",
    "docker:run": "docker run -p 8082:8082 --rm -it --env-file .env playbuddy-scraper",
    "script:branch": "npx tsx ./src/scripts/createBranchLink.ts",
    "script:branch-stats": "npx tsx ./src/scripts/getBranchStats.ts --paginate --page-size=100"
  },
  "author": "",
  "keywords": [],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@google-cloud/logging": "11.0.0",
    "@supabase/supabase-js": "^2.45.2",
    "apify-client": "^2.9.7",
    "axios": "^1.7.2",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.12",
    "cli-table3": "^0.6.5",
    "crypto-random-string": "^5.0.0",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.5.0",
    "express": "4.18.2",
    "googleapis": "^140.0.1",
    "i18n-iso-countries": "^7.12.0",
    "ical-generator": "^7.2.0",
    "jimp": "^1.6.0",
    "luxon": "^3.5.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "openai": "^4.55.4",
    "p-limit": "^6.2.0",
    "p-queue": "^8.1.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.23.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react-tooltip": "^5.27.1",
    "redis": "^4.7.0",
    "request-promise": "^4.2.6",
    "sharp": "^0.34.2",
    "turndown": "^7.2.0",
    "whatsapp-web.js": "^1.26.1-alpha.3",
    "wwebjs-aws-s3": "^1.0.2",
    "wwebjs-google-cloud-storage": "^1.0.9"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.10",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.18.0",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/request-promise": "^4.1.51",
    "@types/supertest": "2.0.16",
    "@types/turndown": "^5.0.5",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "playwright": "^1.55.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "4.6.0",
    "typescript": "^5.9.2"
  }
}
