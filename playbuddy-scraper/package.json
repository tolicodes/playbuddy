{
  "name": "playbuddy-scraper",
  "private": true,
  "description": "Node.js app with TypeScript for Cloud Run",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/app.js",
  "bin": {
    "cmd": "./bin/cmd"
  },
  "files": [
    "dist/**.*"
  ],
  "scripts": {
    "clean": "rimraf dist types",
    "dev": "NODE_ENV=development tsx watch src/app.ts",
    "start": "NODE_ENV=production node ./dist/app.js",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "test": "NODE_ENV=development tsx watch src/tests/tests.ts",
    "fill-missing-data": "tsx src/scripts/fillMissingData.ts",
    "test:watch": "npm test -- --watch",
    "test:watchAll": "npm test -- --watchAll",
    "test:unit": "npm test -- ./src/",
    "test:integration": "npm test -- ./test/",
    "test:smoke": "npm test -- ./test/smoke.test.ts",
    "test:coverage": "npm test -- --coverage",
    "test:serial": "npm test -- --runInBand",
    "deploy": "source .env && gcloud run deploy $SERVICE --region=$REGION --platform managed --allow-unauthenticated --set-env-vars NODE_ENV=production,SUPABASE_ADMIN_KEY=$SUPABASE_ADMIN_KEY,REDIS_PASSWORD=$REDIS_PASSWORD,SUPABASE_HOST=$SUPABASE_HOST --source .",
    "logs": " gcloud beta run services logs read $SERVICE --project=$PROJECT --region=[REGION] --limit=10",
    "logs:tail": "gcloud beta run services logs tail $SERVICE --project=$PROJECT --region=$REGION"
  },
  "author": "",
  "keywords": [],
  "dependencies": {
    "@google-cloud/logging": "11.0.0",
    "@supabase/supabase-js": "^2.45.2",
    "@types/puppeteer": "^7.0.4",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cli-table3": "^0.6.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.5",
    "express": "4.18.2",
    "googleapis": "^140.0.1",
    "ical-generator": "^7.2.0",
    "luxon": "^3.5.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "openai": "^4.55.4",
    "puppeteer": "^22.15.0",
    "qrcode-terminal": "^0.12.0",
    "react-tooltip": "^5.27.1",
    "redis": "^4.7.0",
    "turndown": "^7.2.0",
    "whatsapp-web.js": "^1.25.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.10",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.1.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "2.0.16",
    "@types/turndown": "^5.0.5",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "4.6.0",
    "typescript": "5.3.2"
  }
}
